{
  "cli": {
    "version": ">=16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "credentialsSource": "remote"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "android": {
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
